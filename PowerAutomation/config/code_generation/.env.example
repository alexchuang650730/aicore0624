# KiloCode集成环境变量配置
# PowerAutomation Code Generation MCP Environment Variables

# KiloCode API密钥
KILO_API_KEY=sk-ant-api03-[YOUR_API_KEY_HERE]

# KiloCode服务配置
KILOCODE_URL=http://localhost:8080
KILOCODE_TIMEOUT=30

# 集成策略配置
USE_KILOCODE_FALLBACK=true
QUALITY_THRESHOLD=0.7

# 性能配置
MAX_CONCURRENT_GENERATIONS=5
CACHE_ENABLED=true
CACHE_TTL=3600

# 日志配置
LOG_LEVEL=INFO
LOG_KILOCODE_CALLS=true
LOG_QUALITY_COMPARISONS=true

